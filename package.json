{
  "name": "codepulse-monorepo",
  "version": "1.0.0",
  "description": "High-Reliability Code Execution Engine with React Frontend, Node.js Backend, and High QA Coverage",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "e2e"
  ],
  "scripts": {
    "install-all": "yarn install",
    "dev": "yarn workspaces run dev",
    "build": "yarn workspace @codepulse/frontend build && yarn workspace @codepulse/backend build",
    "build:backend": "yarn workspace @codepulse/backend build",
    "test": "yarn workspaces run test",
    "test:coverage": "yarn workspaces run test:coverage --exclude e2e",
    "lint": "yarn workspaces run lint",
    "format": "yarn workspaces run format"
  },
  "keywords": [
    "code-execution",
    "online-ide",
    "monorepo",
    "react",
    "nodejs",
    "qa",
    "testing"
  ],
  "author": "Patrick Araujo",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "dotenv": "^17.3.1"
  }
}
